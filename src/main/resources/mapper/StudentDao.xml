<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.practice.dao.StudentDao">

    <select id="getById" resultType="com.practice.bean.Student" parameterType="int">
        SELECT *
        FROM student
        WHERE id=#{id}
    </select>

</mapper>